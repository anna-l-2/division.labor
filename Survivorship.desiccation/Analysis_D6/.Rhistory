strain.label <- function(df) {
df %>%
mutate(Strain = case_when(
substr(Well, 1, 1) %in% c("A", "B", "C", "D") ~ "168",
substr(Well, 1, 1) %in% c("E", "F", "G", "H") ~ "SPO",
TRUE ~ "Unknown"
))
}
